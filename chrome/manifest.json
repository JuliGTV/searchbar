{
  "manifest_version": 3,
  "name": "Julius searchbar live",
  "version": "1.0.2",
  "description": "allow use of customisable keywords in the omnibox",
  "omnibox": { "keyword" : "key" },
  "background": {
    "service_worker": "background.js"
  },
  "host_permissions": ["http://127.0.0.1:5000/*"], 
  "action": {
    "default_popup": "popup.html"
  },
  "permissions": ["tabs", "identity", "identity.email"]
}
